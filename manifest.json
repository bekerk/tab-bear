{
    "manifest_version": 3,
    "name": "Tab Bear",
    "version": "1.0.0",
    "description": "Tab Bear is a cool secret thing...",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "storage",
        "tabs",
        "downloads"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["welcome.html"],
            "matches": ["<all_urls>"]
        }
    ],
    "icons": {}
}